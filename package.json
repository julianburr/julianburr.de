{
  "name": "julianburr.de",
  "private": true,
  "description": "Personal Website",
  "version": "1.0.0",
  "author": "Julian Burr <hello@julianburr.de>",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "@svgr/webpack": "^4.1.0",
    "babel-plugin-styled-components": "^1.10.0",
    "dayjs": "^1.8.0",
    "gatsby": "^2.0.76",
    "gatsby-image": "^2.0.20",
    "gatsby-plugin-layout": "^1.0.11",
    "gatsby-plugin-manifest": "^2.0.9",
    "gatsby-plugin-offline": "^2.0.16",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-sharp": "^2.0.14",
    "gatsby-plugin-styled-components": "^3.0.4",
    "gatsby-plugin-svgr": "^2.0.1",
    "gatsby-source-filesystem": "^2.0.17",
    "gatsby-transformer-remark": "^2.2.0",
    "gatsby-transformer-sharp": "^2.1.8",
    "prop-types": "^15.6.2",
    "react": "^16.8.0-alpha.1",
    "react-dom": "^16.8.0-alpha.1",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.3"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "serve": "gatsby build && gatsby serve",
    "clean": "rm -rf ./.cache ./public",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "format": "prettier --write '**/*.{js,jsx,json,css,md}'",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "lint:staged": "lint-staged",
    "deploy": "yarn clean && yarn build && cp now.json ./public/now.json && cd ./public && now --public && now alias && cd -"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.3.2",
    "@commitlint/config-conventional": "^7.3.1",
    "eslint": "^5.12.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.2",
    "prettier-eslint-cli": "^4.7.1"
  },
  "lint-staged": {
    "linters": {
      "**/*.{js,jsx}": [
        "prettier-eslint --write",
        "git add",
        "eslint"
      ],
      "**/*.{json,css,md}": [
        "prettier --write '**/*.{json,css,md}'",
        "git add"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
